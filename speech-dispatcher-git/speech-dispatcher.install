post_install() {
  # Enable socket by default
  systemctl --global enable speech-dispatcher.socket
}

post_upgrade() {
  if (( $(vercmp $2 0.12.0.r50.g488f5d97-1) < 0)); then
    systemctl --global enable speech-dispatcher.socket
  fi
}

pre_remove() {
  systemctl --global disable speech-dispatcher.socket
}
